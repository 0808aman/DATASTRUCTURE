class Node:                      #Define a Node
  def __init__(self, data):
    self.data = data
    self.next = None
   
class LinkedList:                #Define a LinkedList
  def __init__(self):
    self.head = None
    
    
if __name__ = '__main__':       #Prgram Execution Starts here
  
  llist = LinkedList()          #create a empty list
  
  llist.head() = Node(1)        #assigning data
  second = Node(2)
  third = Node(3)
  
llist.head.next = second        #assiging address ( Linking the data )
second.next = third


Print all the elements of the linked list.

'''
class node:
    def __init__(self):
        self.data = None
        self.next = None
'''
class Solution:
    def display(self,node):
        while node:
            print(node.data, end=" ")
            node = node.next
            
            
 
  
